    "@babel/preset-env": "^7.4.3",
    "eslint": "^5.16.0",
    "express": "^4.16.4",
    "rimraf": "^2.6.3",
    "rollup": "^1.8.0",
    "rollup-plugin-babel": "^4.3.2",
    "rollup-plugin-copy": "^0.2.3",
    "rollup-plugin-terser": "^4.0.4"
  },
  "dependencies": {}
