  "main": "./dist/yall.js",
  "module": "./src/yall.mjs",
  "jsnext:main": "./src/yall.mjs",
  "scripts": {
    "clean": "npx rimraf ./dist",
    "lint": "npx eslint ./src/yall.mjs",
